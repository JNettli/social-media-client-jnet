    "husky": "^8.0.0",
    "jest": "^29.2.0",
    "live-server": "^1.2.0",
    "prettier": "^3.3.3",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  }
